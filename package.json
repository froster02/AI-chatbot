{
  "name": "airtist",
  "version": "1.0.0",
  "description": "A modern AI chat interface powered by Mistral-7B",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev:server": "nodemon server/index.js",
    "dev:client": "cd client && npm run dev",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "build": "cd client && npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/froster02/AI-chatbot.git"
  },
  "keywords": [
    "ai",
    "chatbot",
    "mistral",
    "react",
    "nodejs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/froster02/AI-chatbot/issues"
  },
  "homepage": "https://github.com/froster02/AI-chatbot#readme",
  "dependencies": {
    "axios": "^1.7.9",
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "dotenv": "^16.4.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.3",
    "jest": "^29.7.0"
  }
}
